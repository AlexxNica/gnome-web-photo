msgstr ""
"Upotreba: %s [-c CSSFILE] [-d DELAY] [-t TIMEOUT] [--force] [-w WIDTH] [-s "
"THUMBNAILSIZE] [--file] URI|FILE OUTFILE\n"

#: ../src/gnome-web-photo.c:835
#, c-format
msgid ""
"Usage: %s [-c CSSFILE] [-d DELAY] [-t TIMEOUT] [--force] [-w WIDTH] [--print-"
