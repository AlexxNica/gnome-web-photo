#: ../src/main.cpp:146
#, c-format
msgid "Unknown mode '%s'"
msgstr "未知模式 '%s'"

#: ../src/main.cpp:171
#, c-format
msgid "Unknown format '%s'"
msgstr "未知格式 '%s'"

#: ../src/main.cpp:439
#, c-format
msgid "Usage: %s [--mode=photo|thumbnail|print] [...]\n"
msgstr "用法：%s [--mode=photo|thumbnail|print] [...]\n"

#: ../src/main.cpp:443
#, c-format
msgid ""
"Usage: %s [-t TIMEOUT] [--force] [--format FORMAT] [-w WIDTH] [--files] URI|"
"FILE OUTFILE [...]\n"
msgstr ""
"用法：%s [-t TIMEOUT] [--force] [--format FORMAT] [-w WIDTH] [--files] URI|"
"FILE OUTFILE [...]\n"

#: ../src/main.cpp:446
#, c-format
msgid ""
"Usage: %s [-t TIMEOUT] [--force] [-w WIDTH] -s SIZE [--files] URI|FILE "
"OUTFILE [...]\n"
msgstr ""
"用法：%s [-t TIMEOUT] [--force] [-w WIDTH] -s SIZE [--files] URI|FILE "
"OUTFILE [...]\n"

#: ../src/main.cpp:449
#, c-format
msgid ""
"Usage: %s [-t TIMEOUT] [--force] [-w WIDTH] [--print-background] [--files] "
"URI|FILE OUTFILE [...]\n"
msgstr ""
"用法： %s [-t TIMEOUT] [--force] [-w WIDTH] [--print-background] [--files] "
"URI|FILE OUTFILE [...]\n"

#: ../src/main.cpp:468
msgid "Operation mode [photo|thumbnail|print]"
msgstr "操作模式 [photo|thumbnail|print]"

#: ../src/main.cpp:470
msgid "The timeout in seconds, or 0 to disable timeout (default: 60)"
msgstr "此倒计时以秒为单位，或者 0 表示禁用倒计时 (默认：60)"

#: ../src/main.cpp:472
msgid "Force output when timeout expires, even if the page isn't loaded fully"
msgstr "倒计时到期时强制输出，即使页面没有完全加载"

#: ../src/main.cpp:474
msgid "The desired width of the image (default: 1024)"
msgstr "此图片希望的宽度(默认：1024)"

#: ../src/main.cpp:476
msgid "The thumbnail size (default: 256)"
msgstr "缩略图大小(默认：256)"

#: ../src/main.cpp:478
msgid ""
"File format for output. Supported are 'png', 'jpeg' and 'ppm' (default:png)"
msgstr "用于输出的文件格式。支持 'png', 'jpeg' and 'ppm' (默认：png)"

#: ../src/main.cpp:478
msgid "FORMAT"
msgstr "格式"

#: ../src/main.cpp:480
msgid "Print background images and colours (default: false)"
msgstr "打印背景图片和颜色 (默认: false)"

#: ../src/main.cpp:490
msgid "The desired quality of the JPEG image (1 .. 100)"
msgstr "JPEG 希望的质量 (1 ... 100)"

#: ../src/main.cpp:490
msgid "QUALITY"
msgstr "质量"

#: ../src/main.cpp:515
msgid "JPEG options:"
msgstr "JPEG 选项："

#: ../src/main.cpp:516
msgid "JPEG options"
msgstr "JPEG 选项"
